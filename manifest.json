{
    "manifest_version": 3,
    "name": "CookieDance",
    "version": "1.0.0",
    "description" : "A cookie is dancing when you have 2 or more cookies with the same name.",
    "icons": {
        "64": "icons/cookie_64.png",
        "128": "icons/cookie_128.png"
    },
    "author": "Alexis Hayat",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "cookies"
    ]
}